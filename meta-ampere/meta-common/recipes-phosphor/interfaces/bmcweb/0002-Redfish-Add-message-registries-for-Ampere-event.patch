+++ b/redfish-core/include/registries/openbmc_message_registry.hpp
@@ -74,6 +74,74 @@ const Header header = {
     "OpenBMC",
 };
 constexpr std::array registry = {
+    MessageEntry{
+        "AmpereEvent",
+        {
+            "Indicates an Ampere OK event",
+            "%1",
+            "OK",
+            1,
+            {
+                "string",
+            },
+            "None.",
+        }},
+    MessageEntry{
+        "AmpereCritical",
+        {
+            "Indicates an Ampere critical event",
+            "%1: %2",
+            "Critical",
