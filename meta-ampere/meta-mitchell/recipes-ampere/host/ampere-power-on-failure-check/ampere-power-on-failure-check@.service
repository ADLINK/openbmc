[Unit]
Description=Check Power On Sequence status
After=obmc-host-started@%i.target
After=obmc-power-start-pre@%i.target
Conflicts=obmc-host-stop@%i.target
Conflicts=phosphor-reset-host-check@%i.service
OnFailure=obmc-host-quiesce@%i.target
OnFailureJobMode=flush

[Service]
Type=oneshot
ExecStart=/usr/sbin/ampere_power_on_failure_check.sh
